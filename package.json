{
  "name": "apes",
  "version": "0.5.0",
  "description": "A tiny application and HTTP API framework for Express.js.",
  "main": "lib/main",
  "files": [
    "lib",
    "README.md",
    "CHANGELOG.md"
  ],
  "typings": "lib/main",
  "scripts": {
    "postpublish": "git push origin --tags && git push -f origin",
    "pretest": "npm run clean",
    "test": "./node_modules/.bin/mocha --recursive test",
    "posttest": "npm run clean && ./node_modules/.bin/tslint --project test/tsconfig.json -t verbose --type-check .",
    "prepare-coverage": "npm run clean && ./node_modules/.bin/tsc -p test/tsconfig.json && ./node_modules/.bin/istanbul --report=json cover ./node_modules/.bin/_mocha -- --opts test/fixtures --recursive test/",
    "coverage": "npm run prepare-coverage && ./node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "travis-ci": "npm run prepare-coverage && ./node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -t lcovonly | ./node_modules/coveralls/bin/coveralls.js",
    "predocs": "npm run clean && tsc -p tsconfig.json",
    "docs": "./node_modules/.bin/jsdoc -c .jsdoc.json -t ./node_modules/minami -R README.md -r",
    "clean": "rm -rf log test/log && find lib test -name '*.js' -delete -o -name '*.map' -delete"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShogunPanda/apes"
  },
  "keywords": [],
  "author": "Shogun <shogun@cowtech.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShogunPanda/apes/issues"
  },
  "homepage": "https://github.com/ShogunPanda/apes",
  "dependencies": {
    "@types/bluebird": "^3.0.33",
    "@types/body-parser": "^0.0.32",
    "@types/compression": "^0.0.32",
    "@types/express": "^4.0.32",
    "@types/fs-extra": "^0.0.31",
    "@types/node": "^6.0.38",
    "@types/on-headers": "^2.1.28",
    "@types/sprintf-js": "^0.0.26",
    "@types/winston": "^0.0.27",
    "bluebird": "~3.4.6",
    "body-parser": "~1.15.2",
    "compression": "~1.6.2",
    "express": "~4.14.0",
    "fs-extra": "~0.30.0",
    "http2": "^3.3.6",
    "on-headers": "~1.0.1",
    "serve-static": "~1.11.1",
    "sprintf-js": "~1.0.3",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/chai-as-promised": "^0.0.28",
    "@types/chai-http": "^0.0.28",
    "@types/mocha": "^2.2.31",
    "@types/sinon": "^1.16.29",
    "chai": "~3.5.0",
    "chai-as-promised": "~5.3.0",
    "chai-http": "~3.0.0",
    "coveralls": "~2.11.14",
    "istanbul": "~0.4.2",
    "jsdoc": "~3.4.0",
    "minami": "~1.1.1",
    "mocha": "~3.0.2",
    "remap-istanbul": "~0.6.4",
    "sinon": "~1.17.6",
    "ts-node": "~1.3.0",
    "tslint": "~3.15.1",
    "typescript": "^2.1.0-dev.20160919"
  }
}
